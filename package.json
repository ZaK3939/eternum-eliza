{
  "name": "eternum",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --dir ./client dev",
    "build-packages": "pnpm --dir packages/eternum build",
    "build:dotty-deps": "NODE_OPTIONS='--max-old-space-size=8192' pnpm --filter \"@elizaos/core\" --filter \"@elizaos/adapter-postgres\" --filter \"@elizaos/client-auto\" --filter \"@elizaos/client-direct\" --filter \"@elizaos/client-telegram\" --filter \"@elizaos/plugin-bootstrap\" --filter \"@elizaos/plugin-node\" build",
    "start:dotty": "pnpm build:dotty-deps && pnpm --filter dotty start"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "^1.20.0"
    }
  },
  "dependencies": {
    "@huggingface/transformers": "3.0.2",
    "openai": "4.73.0",
    "typescript": "5.4.4",
    "zod": "^3.24.1"
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
